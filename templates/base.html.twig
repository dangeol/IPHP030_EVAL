{# templates/base.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Test Application{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('assets/main.css') }}" rel="stylesheet"/>
    {% endblock %}
</head>
<body>
    {% block headline %}{% endblock %}
    {% block body %}{% endblock %}
    {% block javascripts %}
        <script src="{{ asset('assets/main.js') }}"></script>
    {% endblock %}
</body>
</html>
